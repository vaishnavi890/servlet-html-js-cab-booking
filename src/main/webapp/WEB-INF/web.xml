<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>userController</servlet-name>
    <servlet-class>com.vaishnavi.servlet.cab.booking.controller.UserController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>driverController</servlet-name>
    <servlet-class>com.vaishnavi.servlet.cab.booking.controller.DriverController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>userController</servlet-name>
    <url-pattern>/user-controller</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>driverController</servlet-name>
    <url-pattern>/driver-controller</url-pattern>
  </servlet-mapping>

</web-app>
